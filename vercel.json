{
    "builds": [
        {
            "src": "package.json",
            "use": "@vercel/static-build",
            "config": {
                "distDir": "docs/.vitepress/dist"
            }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/docs/.vitepress/dist/$1"
        }
    ],
    
    "cleanUrls": true ,
    
    "headers": [
        {
        "source": "/assets/(.*)",
        "headers": [
            {
            "key": "Cache-Control",
            "value": "max-age=31536000, immutable"
            }
        ]
        }
    ]
}